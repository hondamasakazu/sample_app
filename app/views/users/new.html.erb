<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="span6 offset3">

    <%= form_for(@user) do |f| %>
      <%= render 'fields', f: f %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.submit "アカウント作成", class: "btn btn-samll btn-primary" %>
    <% end %>

  </div>
</div>
