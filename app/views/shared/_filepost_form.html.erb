<%= form_tag('/file_upload', :multipart => true) do %>
  <br/>
  <p style="font-size: 0.7em; margin-top: 1px; margin-bottom: 5px;">
  	<%= file_field "upload_file", "doc", "size" => 60 %>
  </p>
  <p style="font-size: 0.7em; margin-top: 0px; margin-bottom: 0px;">
  	<%= submit_tag 'アップロード', class: "btn btn-samll btn-primary" %>
  </p>
<% end %>
