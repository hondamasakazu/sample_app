<% provide(:title, "Edit DocMg") %>
<h1>ドキュメント管理項目の更新</h1>

<div class="row">
  <div class="span6 offset3">

    <%= form_for(@document_management) do |f| %>
      <%= render 'fields', f: f %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.submit "保存", class: "btn btn-small btn-primary" %>
    <% end %>

  </div>
</div>